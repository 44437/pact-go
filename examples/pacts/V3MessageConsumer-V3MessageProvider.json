{
  "consumer": {
    "name": "V3MessageConsumer"
  },
  "messages": [
    {
      "contents": {
        "datetime": "2020-01-01",
        "id": 12,
        "lastName": "Sampson",
        "name": "Billy"
      },
      "description": "a user event",
      "matchingRules": {
        "body": {
          "$.datetime": {
            "combine": "AND",
            "matchers": [
              {
                "match": "regex",
                "regex": "[0-9\\-]+"
              }
            ]
          },
          "$.id": {
            "combine": "AND",
            "matchers": [
              {
                "match": "integer"
              }
            ]
          }
        }
      },
      "metadata": {},
      "providerStates": [
        {
          "name": "User with id 127 exists",
          "params": {
            "id": 127
          }
        }
      ]
    }
  ],
  "metadata": {
    "pactRust": {
      "version": "0.9.3"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "V3MessageProvider"
  }
}